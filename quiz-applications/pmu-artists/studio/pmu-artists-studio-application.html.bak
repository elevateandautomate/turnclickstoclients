<script>
    document.addEventListener('DOMContentLoaded', function() {
        const params = new URLSearchParams(window.location.search);
        const firstName = params.get('firstName');
        const greetingElement = document.getElementById('personalized-greeting-pmu-studio');
        
        if (firstName) {
            const decodedName = decodeURIComponent(firstName);
            if (greetingElement && greetingElement.innerHTML.includes('[First Name]')) {
                 greetingElement.innerHTML = greetingElement.innerHTML.replace('[First Name]', `<span class="user-name-emphasis">${decodedName}</span>`);
            } else if (greetingElement) { 
                greetingElement.innerHTML = `Hi <span class="user-name-emphasis">${decodedName}</span>, Ready to Transform Your Studio into a Beacon of Prestige and a Hub of Profitability?`;
            }
        } else {
            const defaultName = "Artist"; 
             if (greetingElement && greetingElement.innerHTML.includes('[First Name]')) {
                greetingElement.innerHTML = greetingElement.innerHTML.replace('[First Name]', `<span class="user-name-emphasis">${defaultName}</span>`);
             } else if (greetingElement) {
                greetingElement.innerHTML = `Hi <span class="user-name-emphasis">${defaultName}</span>, Ready to Transform Your Studio into a Beacon of Prestige and a Hub of Profitability?`;
             }
        }

        const hamburger = document.querySelector('.hamburger');
        const mainNav = document.getElementById('main-nav');
        if (hamburger && mainNav) {
            hamburger.addEventListener('click', () => {
                const isExpanded = hamburger.getAttribute('aria-expanded') === 'true' || false;
                hamburger.setAttribute('aria-expanded', !isExpanded);
                mainNav.classList.toggle('active');
            });
        }

        const faqItems = document.querySelectorAll('.faq-item-pmu-studio');
        faqItems.forEach(item => {
            const question = item.querySelector('.faq-question-pmu-studio');
            question.addEventListener('click', () => {
                const currentlyActive = document.querySelector('.faq-item-pmu-studio.active');
                if (currentlyActive && currentlyActive !== item) {
                    currentlyActive.classList.remove('active');
                }
                item.classList.toggle('active');
            });
        });
    });
</script>

