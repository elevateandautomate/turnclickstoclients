// Track when user views this page
function trackPageView() {
    if (!supabaseClient) return;

    supabaseClient
        .from('tctc_user_flow')
        .insert([{
            user_first_name: window.userParams.firstName,
            user_last_name: window.userParams.lastName,
            user_business_name: window.userParams.businessName,
            action_type: 'page_view',
            page_path: window.location.pathname,
            page_bucket: window.userParams.bucket,
            page_variant: window.userParams.variant,
            page_split: window.userParams.split,
            original_source: window.userParams.source,
            track_source: window.userParams.track_source,
            quiz_score: window.userParams.score,
            full_url: window.location.href,
            timestamp: new Date().toISOString()
        }])
        .then(({ data, error }) => {
            if (error) {
                console.error('Error tracking page view:', error);
            } else {
                console.log('Page view tracked successfully');
            }
        });
}

