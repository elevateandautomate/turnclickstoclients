    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const params = new URLSearchParams(window.location.search);
            const firstName = params.get('firstName');
            const greetingElement = document.getElementById('personalized-greeting');
            const userNameElements = document.querySelectorAll('.user-name-emphasis');

            if (firstName) {
                const decodedName = decodeURIComponent(firstName);
                if (greetingElement) {
                     greetingElement.innerHTML = `Hello <span class="user-name-emphasis">${decodedName}</span>!`;
                }
                userNameElements.forEach(el => {
                    if (el.parentNode.tagName === 'H1' && el.textContent.includes('[First Name]')) {
                         el.textContent = decodedName;
                    } else if (el.textContent === '[First Name]') {
                        el.textContent = decodedName;
                    }
                });
            } else {
                const defaultName = "Clinic Leader";
                 if (greetingElement) {
                    greetingElement.innerHTML = `Hello <span class="user-name-emphasis">${defaultName}</span>!`;
                }
                userNameElements.forEach(el => {
                   if (el.parentNode.tagName === 'H1' && el.textContent.includes('[First Name]')) {
                        el.textContent = defaultName;
                    } else if (el.textContent === '[First Name]') {
                        el.textContent = defaultName;
                    }
                });
            }
            // Hamburger menu toggle
            const hamburger = document.querySelector('.hamburger');
            const mainNav = document.getElementById('main-nav');
            if (hamburger && mainNav) {
                hamburger.addEventListener('click', () => {
                    const isExpanded = hamburger.getAttribute('aria-expanded') === 'true' || false;
                    hamburger.setAttribute('aria-expanded', !isExpanded);
                    mainNav.classList.toggle('active');
                });
            }
        });
    </script>

