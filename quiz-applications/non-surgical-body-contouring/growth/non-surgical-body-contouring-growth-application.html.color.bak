    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const params = new URLSearchParams(window.location.search);
            const firstName = params.get('firstName');
            const greetingElement = document.getElementById('personalized-greeting-growth');
            
            if (firstName) {
                const decodedName = decodeURIComponent(firstName);
                if (greetingElement && greetingElement.innerHTML.includes('[First Name]')) {
                     greetingElement.innerHTML = greetingElement.innerHTML.replace('[First Name]', `<span class="user-name-emphasis">${decodedName}</span>`);
                } else if (greetingElement) { 
                    greetingElement.innerHTML = `Ready to Scale Your Success, <span class="user-name-emphasis">${decodedName}</span>?`;
                }
            } else {
                const defaultName = "Clinic Owner"; 
                 if (greetingElement && greetingElement.innerHTML.includes('[First Name]')) {
                    greetingElement.innerHTML = greetingElement.innerHTML.replace('[First Name]', `<span class="user-name-emphasis">${defaultName}</span>`);
                 } else if (greetingElement) {
                    greetingElement.innerHTML = `Ready to Scale Your Success, <span class="user-name-emphasis">${defaultName}</span>?`;
                 }
            }

            const hamburger = document.querySelector('.hamburger');
            const mainNav = document.getElementById('main-nav');
            if (hamburger && mainNav) {
                hamburger.addEventListener('click', () => {
                    const isExpanded = hamburger.getAttribute('aria-expanded') === 'true' || false;
                    hamburger.setAttribute('aria-expanded', !isExpanded);
                    mainNav.classList.toggle('active');
                });
            }

            const faqItems = document.querySelectorAll('.faq-item');
            faqItems.forEach(item => {
                const question = item.querySelector('.faq-question');
                question.addEventListener('click', () => {
                    const currentlyActive = document.querySelector('.faq-item.active');
                    if (currentlyActive && currentlyActive !== item) {
                        currentlyActive.classList.remove('active');
                    }
                    item.classList.toggle('active');
                });
            });
        });
    </script>

