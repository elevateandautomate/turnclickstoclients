    <script>
    const hamburger = document.querySelector('.hamburger');
    const navUl = document.getElementById('main-nav');
    if (hamburger && navUl) {
        function closeAllDropdowns() {
            document.querySelectorAll('.dropdown-parent').forEach(li => li.classList.remove('open'));
        }
        hamburger.addEventListener('click', () => {
            const isExpanded = hamburger.getAttribute('aria-expanded') === 'true';
            hamburger.setAttribute('aria-expanded', !isExpanded);
            navUl.classList.toggle('active');
            if (!isExpanded) {
                hamburger.setAttribute('aria-label', 'Close navigation');
            } else {
                hamburger.setAttribute('aria-label', 'Open navigation');
                closeAllDropdowns(); // Close dropdowns when closing mobile menu
            }
        });

        document.querySelectorAll('.dropdown-parent > a').forEach(anchor => {
            anchor.addEventListener('click', (e) => {
                if (window.innerWidth <= 768 && e.target.classList.contains('dropdown-arrow')) {
                     e.preventDefault(); 
                     const parentLi = anchor.closest('.dropdown-parent');
                     parentLi.classList.toggle('open');
                     document.querySelectorAll('.dropdown-parent').forEach(li => {
                         if (li !== parentLi) {
                             li.classList.remove('open');
                         }
                     });
                }
            });
        });

         document.addEventListener('click', (e) => {
            if (!e.target.closest('.dropdown-parent') && !e.target.closest('.hamburger')) {
                closeAllDropdowns();
            }
            if (navUl.classList.contains('active') && !e.target.closest('nav') && !e.target.closest('.hamburger')) {
                hamburger.setAttribute('aria-expanded', 'false');
                hamburger.setAttribute('aria-label', 'Open navigation');
                navUl.classList.remove('active');
                closeAllDropdowns();
            }
        });
    }
</script>

