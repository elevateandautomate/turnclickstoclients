<script src="../script.js"></script>
<script>
    // Mobile nav toggle
    const hamburger = document.querySelector('.hamburger');
    const navUl = document.getElementById('main-nav');
    if (hamburger && navUl) {
      function closeAllDropdowns() {
        document.querySelectorAll('.dropdown-parent').forEach(li => li.classList.remove('open'));
      }
      hamburger.style.display = 'block';
      hamburger.addEventListener('click', function() {
        const open = navUl.classList.toggle('open');
        hamburger.setAttribute('aria-expanded', open);
        if (!open) closeAllDropdowns();
      });
      navUl.querySelectorAll('.dropdown-parent > a').forEach(link => {
        link.addEventListener('click', function(e) {
          if (window.innerWidth <= 900) {
            e.preventDefault();
            const parent = this.parentElement;
            const isOpen = parent.classList.toggle('open');
            navUl.querySelectorAll('.dropdown-parent').forEach(li => {
              if (li !== parent) li.classList.remove('open');
            });
          }
        });
      });
      navUl.querySelectorAll('a').forEach(link => {
        link.addEventListener('click', function() {
          if (window.innerWidth <= 900) {
            navUl.classList.remove('open');
            hamburger.setAttribute('aria-expanded', 'false');
            closeAllDropdowns();
          }
        });
      });
    }

    // Chat Widget Script
    const chatButton = document.getElementById('chat-widget-button');
    const chatModal = document.getElementById('chat-modal');
    const chatCloseButton = document.getElementById('chat-modal-close');
    const chatForm = document.getElementById('chat-contact-form');
    const chatSuccessMessage = document.getElementById('chat-success-message');

    if (chatButton && chatModal) {
        chatButton.addEventListener('click', () => {
            let wasOpen = chatModal.classList.contains('modal-visible');
            chatModal.classList.toggle('modal-visible');
            chatModal.classList.toggle('modal-hidden');
            if (!wasOpen) { 
                chatForm.style.display = 'block';
                chatForm.reset(); 
                chatSuccessMessage.style.display = 'none';
             }
        });
    }

    if (chatCloseButton && chatModal) {
        chatCloseButton.addEventListener('click', () => {
            chatModal.classList.add('modal-hidden');
            chatModal.classList.remove('modal-visible');
        });
    }
    if (chatModal) { 
        window.addEventListener('click', (event) => {
            if (event.target === chatModal) {
                chatModal.classList.add('modal-hidden');
                chatModal.classList.remove('modal-visible');
            }
        });
    }
    if (chatForm) {
        chatForm.addEventListener('submit', function(event) {
            event.preventDefault();
            let isValid = true;
            chatForm.querySelectorAll('[required]').forEach(input => {
                if (!input.value.trim()) { 
                    isValid = false;
                    input.style.borderColor = 'red'; 
                } else {
                    input.style.borderColor = '#ccc'; 
                }
            });

            if (isValid) {
                console.log('Chat form submitted'); 
                chatForm.style.display = 'none';
                chatSuccessMessage.style.display = 'block';
            }
        });
    }
</script>
