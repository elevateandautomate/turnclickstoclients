document.addEventListener('DOMContentLoaded', function() {
    const currentParams = new URLSearchParams(window.location.search);
    const firstName = currentParams.get('firstName');
    const greetingElement = document.getElementById('personalized-greeting-pmu-growth-a');

    if (firstName) {
        const decodedName = decodeURIComponent(firstName);
        if (greetingElement && greetingElement.innerHTML.includes('[First Name]')) {
             greetingElement.innerHTML = greetingElement.innerHTML.replace('[First Name]', `<span class="user-name-emphasis">${decodedName}</span>`);
        } else if (greetingElement) {
            greetingElement.innerHTML = `Hi <span class="user-name-emphasis">${decodedName}</span>, Your Quiz Results Reveal A Powerful Opportunity`;
        }
    } else {
        const defaultName = "Artist";
         if (greetingElement && greetingElement.innerHTML.includes('[First Name]')) {
            greetingElement.innerHTML = greetingElement.innerHTML.replace('[First Name]', `<span class="user-name-emphasis">${defaultName}</span>`);
         } else if (greetingElement) {
            greetingElement.innerHTML = `Hi <span class="user-name-emphasis">${defaultName}</span>, Your Quiz Results Reveal A Powerful Opportunity`;
         }
    }

    const hamburger = document.querySelector('.hamburger');
    const mainNav = document.getElementById('main-nav');
    if (hamburger && mainNav) {
        hamburger.addEventListener('click', () => {
            const isExpanded = hamburger.getAttribute('aria-expanded') === 'true' || false;
            hamburger.setAttribute('aria-expanded', !isExpanded);
            mainNav.classList.toggle('active');
        });
    }

    // Chat Modal functionality
    const chatButton = document.getElementById('chat-widget-button');
    const chatModal = document.getElementById('chat-modal');
    const closeButton = document.getElementById('chat-modal-close');
    const chatForm = document.getElementById('chat-contact-form');
    const successMessage = document.getElementById('chat-success-message');

    if (chatButton && chatModal) {
        chatButton.addEventListener('click', function(e) {
            e.preventDefault();
            chatModal.classList.remove('modal-hidden');
            chatModal.classList.add('modal-visible');
            document.body.style.overflow = 'hidden'; // Prevent background scrolling

            // Ensure the modal content is scrollable and visible
            setTimeout(function() {
                const modalBody = document.querySelector('.chat-modal-body');
                if (modalBody) {
                    modalBody.scrollTop = 0; // Start at the top
                }

                // Make sure the submit button is visible by scrolling to it if needed
                const submitButton = document.querySelector('#chat-contact-form button');
                if (submitButton) {
                    submitButton.scrollIntoView({ behavior: 'smooth', block: 'nearest' });
                }
            }, 300); // Small delay to ensure modal is fully visible
        });
    }

    if (closeButton && chatModal) {
        closeButton.addEventListener('click', function(e) {
            e.preventDefault();
            chatModal.classList.remove('modal-visible');
            chatModal.classList.add('modal-hidden');
            document.body.style.overflow = ''; // Restore scrolling
        });
    }

    // Close modal when clicking outside content
    if (chatModal) {
        chatModal.addEventListener('click', function(e) {
            if (e.target === chatModal) {
                chatModal.classList.remove('modal-visible');
                chatModal.classList.add('modal-hidden');
                document.body.style.overflow = ''; // Restore scrolling
            }
        });
    }

    // Close modal with Escape key
    document.addEventListener('keydown', function(event) {
        if (event.key === 'Escape' && chatModal && chatModal.classList.contains('modal-visible')) {
            chatModal.classList.remove('modal-visible');
            chatModal.classList.add('modal-hidden');
            document.body.style.overflow = ''; // Restore scrolling
        }
    });

    // Form submission
    if (chatForm && successMessage) {
        chatForm.addEventListener('submit', function(e) {
            e.preventDefault();
            // Here you would normally send data to server
            // For demo purposes, just show success message
            chatForm.style.display = 'none';
            successMessage.style.display = 'block';
        });
    }
});
</script>

